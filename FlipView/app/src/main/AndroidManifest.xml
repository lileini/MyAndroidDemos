<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eric.flipview">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".FlipTextViewActivity"/>
        <activity android:name=".FlipButtonActivity"/>
        <activity android:name=".FlipComplexLayoutActivity"/>
        <activity android:name=".FlipDeleteAdapterActivity"/>
        <activity android:name=".FlipAsyncContentActivity"/>
        <activity android:name=".FlipTextViewAltActivity"/>
        <activity android:name=".FlipHorizontalLayoutActivity"/>
        <activity android:name=".FlipTextViewXmlActivity"/>
        <activity android:name=".Issue5Activity"/>
        <activity android:name=".FlipFragmentActivity"/>
        <activity android:name=".FlipDynamicAdapterActivity"/>
        <activity android:name=".FlipWebViewActivity"/>
        <activity android:name=".Issue51Activity"/>
    </application>

</manifest>